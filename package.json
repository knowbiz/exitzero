  "main": "index.js",
  "scripts": {
    "test": "standard && node test/test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "keywords": [
    "cli",
    "nofail",
    "failonerror",
    "fail",
    "on",
    "error",
    "task",
    "build"
  ],
  "author": "Dan Flettre",
  "license": "ISC",
  "devDependencies": {
    "condition-circle": "^1.5.0",
    "nixt": "^0.4.1",
    "semantic-release": "^8.2.0",
    "standard": "*",
    "tape": "^3.2.0"
  },
  "directories": {
    "test": "test"
  },
  "bin": {
    "exitzero": "./index.js"
  },
  "dependencies": {
    "cross-spawn": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knowbiz/exitzero.git"
  },
  "bugs": {
    "url": "https://github.com/Flet/exitzero/issues"
  },
  "homepage": "https://github.com/Flet/exitzero",
